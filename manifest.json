{
  "manifest_version": 2,
  "name": "Paper Auto TOC",
  "description": "Paper Auto TOC",
  "version": "1.0",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_title": "paper-auto-toc"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["https://paper.dropbox.com/*"],
    "run_at": "document_end",
    "js": ["contentScript.js"]
  }]
}